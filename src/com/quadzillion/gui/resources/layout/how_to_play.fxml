<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>
<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.quadzillion.gui.controller.HowToPlayController">
    <children>
        <Label layoutX="527.0" layoutY="35.0" text="How to Play"/>
        <Button fx:id="btnStop" layoutX="322.0" layoutY="646.0" mnemonicParsing="false" onAction="#onClickStop"
                prefHeight="50.0" prefWidth="50.0"/>
        <Button fx:id="btnPlay" layoutX="255.0" layoutY="646.0" mnemonicParsing="false" onAction="#onClickPlay"
                prefHeight="50.0" prefWidth="50.0"/>
        <Button fx:id="backBtn" styleClass="button-back" layoutX="14.0" layoutY="14.0" prefWidth="25" prefHeight="25"
                mnemonicParsing="false" onAction="#onReturnToMainMenuButtonClicked"/>
        <MediaView fx:id="mediaView" fitHeight="720.0" fitWidth="1150.0" layoutX="140.0" layoutY="27.0"/>
    </children>
</AnchorPane>

